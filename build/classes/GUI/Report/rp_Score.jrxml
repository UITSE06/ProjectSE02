	<queryString>
		<![CDATA[begin
-- Load bang diem
select distinct MONHOC.MaMonHoc, MaMonHocLT, TenMonHoc, LOAIMONHOC.TenLoai, (MONHOC.SoTiet/LOAIMONHOC.SoTietQuyDoi) as SoTinChi, BANGDIEM.DiemTB
	from BANGDIEM 
	join MONHOC on MONHOC.MaMonHoc = BANGDIEM.MaMonHoc	
	join LOAIMONHOC on MONHOC.MaLoaiMH = LOAIMONHOC.MaLoaiMH
	where MSSV = $P{mssv} and MaHKNH = ((select MaHKNH from HOCKINAMHOC where Nam1 = $P{nam1} and Nam2 = $P{nam2} and HocKi = $P{hockyRP}))
end
]]>
