	<queryString>
		<![CDATA[begin
Declare @hk nvarchar(10);
IF (($P{rpSemeter} IS NULL) OR ($P{rpSemeter}=''))
Begin
set @hk='';
end
else
Begin
	set @hk=CAST($P{rpSemeter} as NVarCHAR(10));
end
Declare @y1 nvarchar(10);
IF (($P{rpYear1} IS NULL) OR ($P{rpYear1}=''))
Begin
set @y1='';
end
else
Begin
	set @y1=CAST($P{rpYear1} as NVarCHAR(10));
end

Declare @y2 nvarchar(10);
IF (($P{rpYear2} IS NULL) OR ($P{rpYear2}=''))
Begin
set @y2='';
end
else
Begin
	set @y2=CAST($P{rpYear2} as NVarCHAR(10));
end

select PHIEUTHU.MaPhieuThu, NHANVIEN.TenNV, PHIEUTHU.NgayThu, PHIEUTHU.SoTienThu, PHIEUTHU.TienConNo

from PHIEUTHU
LEFT JOIN PHIEUDANGKY ON PHIEUDANGKY.MaPhieuDK = PHIEUTHU.MaPhieuDK
LEFT JOIN SINHVIEN ON SINHVIEN.MSSV = PHIEUDANGKY.MSSV
LEFT join NHANVIEN ON NHANVIEN.MaNV = PHIEUTHU.MaNV
 join HOCKINAMHOC on PHIEUDANGKY.MaHKNH = HOCKINAMHOC.MaHKNH
WHERE SINHVIEN.MSSV=$P{rpIdStudent} AND HOCKINAMHOC.HocKi like '%'+@hk+'' and HOCKINAMHOC.Nam1 like '%'+@y1+'%' and HOCKINAMHOC.Nam2 like '%'+@y2+'%'
end]]>
	</queryString>
